{
  "name": "Omni GitHub",
  "description": "Quickly access your Github repos from your browser omnibox",
  "manifest_version": 2,
  "version": "1.0.0",
  "short_name": "GitHub Repos:",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "homepage_url": "https://github.com/firstandthird/omni-github",
  "author": "First+Third <code@firstandthird.com> (http://firstandthird.com/)",
  "offline_enabled": false,
  "permissions": [
    "tabs",
    "alarms",
    "storage",
    "notifications",
    "contextMenus",
    "contentSettings",
    "https://api.github.com/user/repos"
  ],
  "browser_action": {
    "default_title": "Your browser action title"
  },
  "omnibox": {
    "keyword": "gh"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "src/background/background.js"
    ]
  },
  "options_ui": {
    "page": "src/options/options.html",
    "chrome_style": true
  }
}
